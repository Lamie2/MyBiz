@model Service
@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <div class="row">

        <div class="col-md-10 mx-auto border-dark">
            <form asp-action="create" method="post">
                <div class=" mb-3">
                    <label asp-for="Title" class="form-label">Title</label>
                    <input asp-for="Title" type="text" class="form-control">
                    <span class="text-danger" asp-validation-for="Title"></span>
                </div>
                <div class=" mb-3">
                    <label asp-for="Desc" class="form-label">Desc</label>
                    <input asp-for="Desc" type="text" class="form-control">
                    <span class="text-danger" asp-validation-for="Desc"></span>
                </div>
                <div class=" mb-3">
                    <label asp-for="Icon" class="form-label">Icon</label>
                    <input asp-for="Icon" type="text" class="form-control">
                    <span class="text-danger" asp-validation-for="Icon"></span>
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>


